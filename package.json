{
  "name": "skin-maker",
  "displayName": "Skin Maker",
  "description": "VS Code color theme creator ",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.39.0"
  },
  "categories": [
    "Themes"
  ],
  "activationEvents": [
    "onCommand:extension.createSkin"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createSkin",
        "title": "Create Skin"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "color-thief": "^2.2.5",
    "colorthief": "^2.3.0",
    "get-image-colors": "^2.0.1"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.2",
    "@types/vscode": "^1.44.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "tslint": "^6.1.1",
    "vscode-test": "^1.3.0"
  }
}
