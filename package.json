{
  "name": "vscode-extension-starter",
  "displayName": "VSCode Extension Starter",
  "description": "Template for VSCode Extension",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.39.0"
  },
  "categories": [
    "Themes"
  ],
  "activationEvents": [
    "onCommand:extension.exampleCommand"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.exampleCommand",
        "title": "Example Command"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.2",
    "@types/vscode": "^1.44.0",
    "glob": "^7.1.6",
    "mocha": "^10.1.0",
    "typescript": "^3.8.3",
    "tslint": "^6.1.1",
    "vscode-test": "^1.3.0"
  }
}
